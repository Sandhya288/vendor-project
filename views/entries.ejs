<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>About</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% entries.forEach((entry) => { %>
            <tr>
                <td>
                    <%= entry.name %>
                </td>
                <td>
                    <%= entry.email %>
                </td>
                <td>
                    <%= entry.about %>
                </td>
                <td>
                    <form action="/website/create" method="POST" target="_blank">
                        <input type="hidden" name="name" value="<%= entry.name %>" />
                        <input type="hidden" name="email" value="<%= entry.email %>" />
                        <input type="hidden" name="about" value="<%= entry.about %>" />
                        <button type="submit" class="make-website">Make Website</button>
                    </form>
                </td>
            </tr>
            <% }); %>
    </tbody>
</table>